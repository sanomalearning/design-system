{
  "name": "@slds/docs",
  "description": "SLDS Documentation",
  "version": "1.0.0",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "watch:sass": "npx sass --watch src/styles:public/css",
    "build:sass": "npx sass src/styles:public/css",
    "watch:eleventy": "npx @11ty/eleventy --input=src --serve --config=.eleventy.cjs",
    "build:eleventy": "npx @11ty/eleventy --input=src --config=.eleventy.cjs",
    "start": "npm-run-all build:sass --parallel watch:sass watch:eleventy",
    "build": "npm-run-all build:sass build:eleventy",
    "debug": "DEBUG=* npx @11ty/eleventy --input=src",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "license": "????",
  "devDependencies": {
    "@11ty/eleventy": "^1.0.2",
    "@11ty/eleventy-navigation": "^0.3.5",
    "@11ty/eleventy-plugin-syntaxhighlight": "^4.1.0"
  },
  "dependencies": {
    "@lit-labs/eleventy-plugin-lit": "^0.2.3",
    "@webcomponents/template-shadowroot": "^0.1.0",
    "lit": "^2.3.1",
    "markdown-it": "^13.0.1",
    "markdown-it-anchor": "^8.6.5",
    "npm-run-all": "^4.1.5",
    "slugify": "^1.6.5",
    "typescript": "^4.8.2"
  }
}
